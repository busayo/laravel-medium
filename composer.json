{
    "name": "busayo/laravel-medium",
    "description": "A Medium Bridge for Laravel 5",
    "keywords": [
        "busayo",
        "laravel-medium",
        "laravel",
        "medium",
        "bridge",
        "package",
        "php"
    ],
    "homepage": "https://twitter.com/unicodeveloper",
    "license": "MIT",
    "authors": [
        {
            "name": "busayo",
            "email": "prosperotemuyiwa@gmail.com",
            "homepage": "https://twitter.com/unicodeveloper",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Busayo\\Medium\\": "src"
        }
    },
    "autoload-dev": {
